{
  "table": {
    "name": "$EXODUS_TABLE",
    "available_regions": [
      "us-east-1"
    ]
  },
  "config_table": {
    "name": "$EXODUS_CONFIG_TABLE",
    "available_regions": [
      "us-east-1"
    ]
  },
  "config_cache_ttl": $EXODUS_CONFIG_CACHE_TTL,
  "cookie_ttl": $EXODUS_COOKIE_TTL,
  "headers": {
    "max_age": $EXODUS_HEADERS_MAX_AGE
  },
  "secret_arn": "$EXODUS_SECRET_ARN",
  "key_id": "$EXODUS_KEY_ID",
  "lambda_version": "$EXODUS_LAMBDA_VERSION",
  "index_filename": "$EXODUS_INDEX_FILENAME",
  "logging": {
    "version": 1,
    "incremental": true,
    "formatters": {
      "default": {}
    },
    "loggers": {
      "origin-response": {
        "level": "$ORIGIN_RESPONSE_LOGGER_LEVEL"
      },
      "origin-request": {
        "level": "$ORIGIN_REQUEST_LOGGER_LEVEL"
      },
      "default": {
        "level": "WARNING"
      }
    }
  }
}
